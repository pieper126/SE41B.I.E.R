<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:orientation="vertical" android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:weightSum="1">

    <RelativeLayout
        android:layout_width="fill_parent"
        android:layout_height="480dp"
        android:layout_weight="1.18"
        android:id="@+id/or">


        <RelativeLayout
            android:layout_width="fill_parent"
            android:layout_height="55dp"
            android:background="#ffd5d6d6"
            android:layout_below="@+id/relativeLayout"
            android:layout_alignParentStart="true"
            android:layout_marginTop="10dp"
            android:id="@+id/buttons"
            android:onClick="onBarClicked">

            <TextView
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:textAppearance="?android:attr/textAppearanceLarge"
                android:text="Kroeg 2"
                android:id="@+id/textView8"
                android:layout_marginStart="50dp"
                android:layout_centerVertical="true"
                android:layout_alignParentStart="true" />

            <TextView
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:textAppearance="?android:attr/textAppearanceMedium"
                android:text="826m"
                android:id="@+id/textView9"
                android:layout_centerVertical="true"
                android:layout_centerHorizontal="true" />

            <RatingBar
                style="?android:attr/ratingBarStyleSmall"
                android:layout_width="79dp"
                android:layout_height="wrap_content"
                android:id="@+id/ratingBar2"
                android:rating="3.7"
                android:numStars="5"
                android:layout_alignBottom="@+id/textView9"
                android:layout_alignParentEnd="true"
                android:layout_marginEnd="42dp" />
        </RelativeLayout>

        <RelativeLayout
            android:layout_width="fill_parent"
            android:layout_height="55dp"
            android:layout_centerHorizontal="true"
            android:layout_marginTop="10dp"
            android:background="#ffd5d6d6"
            android:id="@+id/relativeLayout"
            android:onClick="onBarClicked">

            <TextView
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:textAppearance="?android:attr/textAppearanceLarge"
                android:text="Kroeg 1"
                android:id="@+id/textView6"
                android:layout_marginStart="50dp"
                android:layout_centerVertical="true"
                android:layout_alignParentStart="true" />

            <TextView
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:textAppearance="?android:attr/textAppearanceMedium"
                android:text="83m"
                android:id="@+id/textView7"
                android:layout_centerVertical="true"
                android:layout_centerHorizontal="true" />

            <RatingBar
                android:layout_width="79dp"
                android:layout_height="wrap_content"
                android:id="@+id/ratingBar"
                android:rating="2.74"
                style="?android:attr/ratingBarStyleSmall"
                android:numStars="5"
                android:layout_alignBottom="@+id/textView7"
                android:layout_alignParentEnd="true"
                android:layout_marginEnd="42dp" />
        </RelativeLayout>

        <RelativeLayout
            android:layout_width="fill_parent"
            android:layout_height="55dp"
            android:background="#ffd5d6d6"
            android:id="@+id/relativeLayout2"
            android:layout_below="@+id/buttons"
            android:layout_alignParentStart="true"
            android:layout_marginTop="10dp"
            android:onClick="onBarClicked">

            <TextView
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:textAppearance="?android:attr/textAppearanceLarge"
                android:text="Kroeg 3"
                android:id="@+id/textView10"
                android:layout_marginStart="50dp"
                android:layout_centerVertical="true"
                android:layout_alignParentStart="true" />

            <TextView
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:textAppearance="?android:attr/textAppearanceMedium"
                android:text="666m"
                android:id="@+id/textView11"
                android:layout_centerVertical="true"
                android:layout_centerHorizontal="true" />

            <RatingBar
                style="?android:attr/ratingBarStyleSmall"
                android:layout_width="79dp"
                android:layout_height="wrap_content"
                android:id="@+id/ratingBar3"
                android:rating="1.4"
                android:numStars="5"
                android:layout_marginEnd="42dp"
                android:layout_centerVertical="true"
                android:layout_alignParentEnd="true" />
        </RelativeLayout>

        <RelativeLayout
            android:layout_width="fill_parent"
            android:layout_height="55dp"
            android:background="#ffd5d6d6"
            android:id="@+id/relativeLayout3"
            android:layout_below="@+id/relativeLayout2"
            android:layout_alignParentStart="true"
            android:layout_marginTop="10dp"
            android:onClick="onBarClicked">

            <TextView
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:textAppearance="?android:attr/textAppearanceLarge"
                android:text="Kroeg 4"
                android:id="@+id/textView12"
                android:layout_marginStart="50dp"
                android:layout_centerVertical="true"
                android:layout_alignParentStart="true" />

            <TextView
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:textAppearance="?android:attr/textAppearanceMedium"
                android:text="69m"
                android:id="@+id/textView13"
                android:layout_centerVertical="true"
                android:layout_centerHorizontal="true" />

            <RatingBar
                style="?android:attr/ratingBarStyleSmall"
                android:layout_width="79dp"
                android:layout_height="wrap_content"
                android:id="@+id/ratingBar4"
                android:rating="0"
                android:numStars="5"
                android:layout_marginEnd="42dp"
                android:layout_centerVertical="true"
                android:layout_alignParentEnd="true" />
        </RelativeLayout>

    </RelativeLayout>

    <LinearLayout
        android:orientation="horizontal"
        android:layout_width="fill_parent"
        android:layout_height="70dp"
        android:baselineAligned="false"
        android:weightSum="1"
        android:layout_margin="0dp"
        android:layout_marginLeft="0dp"
        android:layout_marginTop="0dp"
        android:layout_marginRight="0dp"
        android:layout_marginBottom="0dp">

        <ImageButton
            android:layout_width="0dp"
            android:layout_height="match_parent"
            android:id="@+id/btnSocial"
            android:layout_gravity="center_vertical"
            android:clickable="false"
            android:src="@drawable/socialicon"
            android:scaleType="fitCenter"
            android:layout_weight="0.33"
            android:onClick="onSocialClickedResults" />

        <ImageButton
            android:layout_width="0dp"
            android:layout_height="match_parent"
            android:id="@+id/btnBack"
            android:layout_weight="0.33"
            android:layout_gravity="center_vertical"
            android:src="@drawable/back"
            android:scaleType="fitCenter"
            android:focusableInTouchMode="false"
            android:onClick="onBackClicked" />

        <ImageButton
            android:layout_width="0dp"
            android:layout_height="match_parent"
            android:id="@+id/btnSettings"
            android:layout_weight="0.33"
            android:layout_gravity="center_vertical"
            android:src="@drawable/wrench"
            android:scaleType="fitCenter"
            android:onClick="onContactsClicked" />
    </LinearLayout>


</LinearLayout>
